<!--pages/home/components/backgroundaudio/backgroundaudio.wxml-->
<view class="play-icon-wrap" bindtap="toggleAudioPlay">
  <view class="rotate-wrap" style="transform: {{rotateDeg}};">
    <image class="playing playing-hook" hidden="{{isPaused}}" src="data:image/png;base64,"></image>
    <image class="pause" hidden="{{!isPaused}}" src="data:image/png;base64,"></image>
  </view>
</view>
<audio src="{{bgUrl}}" id="myAudio"></audio>